{
   "display": "Next.js",
   "extends": "../../tsconfig.json",
   "compilerOptions": {
      "plugins": [{ "name": "next" }],
      "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": false,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "incremental": true,
      "esModuleInterop": true,
      "module": "esnext",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "baseUrl": ".",
      "paths": {
         "~/*": ["./src/*"]
      },
      "plugins": [{ "name": "next" }]
   },
   "exclude": [],
   "include": [
      "next-env.d.ts",
      "**/*.ts",
      "**/*.tsx",
      "**/*.cjs",
      "**/*.mjs",
      ".next/types/**/*.ts"
   ]
}
